                instance_create(0, 0, Shop);
            }
        }
        
        else if (b_menuIsOpen == true)
        {
            b_menuIsOpen = false;
            audio_play_sound(snd_door_slam, 1, false);
            if (instance_exists(Shop))
            {
                with (Shop)
