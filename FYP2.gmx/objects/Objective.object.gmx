if (b_shopIsOpen == true)
{
    if (distance_to_object(Player) &lt; 20  &amp;&amp; Player.OPEN &amp;&amp;
        Player.b_deadAnim == false &amp;&amp; Player.b_isDead == false)
    {
        if (b_menuIsOpen == false)
